{
  "name": "wongpinter/google-client",
  "description": "Laravel Google Client SDK Wrapper",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "illuminate/support": "5.5.x",
    "google/apiclient": "^2.0"
  },
  "authors": [
    {
      "name": "WongPinter",
      "email": "root@wongpinter.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wongpinter\\GoogleClient\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Wongpinter\\GoogleClient\\GoogleClientServiceProvider"
      ],
      "aliases": {
        "GoogleClient": "Wongpinter\\GoogleClient\\Facade\\GoogleClientFacade"
      }
    }
  }
}
